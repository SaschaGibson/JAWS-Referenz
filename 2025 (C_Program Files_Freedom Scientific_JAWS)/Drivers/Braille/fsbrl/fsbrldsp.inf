%USB\VID_0F4E&PID_01FF.DeviceDesc%=FSBRLDSP.Dev.ntx86,USB\VID_0F4E&PID_01FF
%USB\VID_0F4E&PID_FFFF.DeviceDesc%=FSBRLDSP.Dev.ntx86,USB\VID_0F4E&PID_FFFF
%USB\VID_0F4E&PID_0100.DeviceDesc%=FSBRLDSP.Dev.ntx86,USB\VID_0F4E&PID_0100

[FreedomScientificSection.ntamd64]
%USB\VID_0F4E&PID_0110.DeviceDesc%=FSBRLDSP.Dev.ntamd64,USB\VID_0F4E&PID_0110
%USB\VID_0F4E&PID_0111.DeviceDesc%=FSBRLDSP.Dev.ntamd64,USB\VID_0F4E&PID_0111
%USB\VID_0F4E&PID_0112.DeviceDesc%=FSBRLDSP.Dev.ntamd64,USB\VID_0F4E&PID_0112
%USB\VID_0F4E&PID_0114.DeviceDesc%=FSBRLDSP.Dev.ntamd64,USB\VID_0F4E&PID_0114
%USB\VID_0F4E&PID_01FF.DeviceDesc%=FSBRLDSP.Dev.ntamd64,USB\VID_0F4E&PID_01FF
%USB\VID_0F4E&PID_FFFF.DeviceDesc%=FSBRLDSP.Dev.ntamd64,USB\VID_0F4E&PID_FFFF
%USB\VID_0F4E&PID_0100.DeviceDesc%=FSBRLDSP.Dev.ntamd64,USB\VID_0F4E&PID_0100

[DestinationDirs]
DefaultDestDir=10,System32\Drivers
;InfCopyFiles        = 17                    ; %WinDir%\Inf

[FSBRLDSP.Dev.ntx86]
CopyFiles=DriverCopyFiles
AddReg=FSBRLDSP.AddRegNt

[FSBRLDSP.Dev.ntx86.Services]
Addservice = FSBRLDSP, 0x00000002, FSBRLDSP.AddService

[FSBRLDSP.Dev.ntamd64]
CopyFiles=DriverCopyFiles
AddReg=FSBRLDSP.AddRegNt

[FSBRLDSP.Dev.ntamd64.Services]
Addservice = FSBRLDSP, 0x00000002, FSBRLDSP.AddService

[FSBRLDSP.AddService]
DisplayName    = %FSBRLDSP.SvcDesc%
ServiceType    = 1                  ; SERVICE_KERNEL_DRIVER
StartType      = 3                  ; SERVICE_DEMAND_START
ErrorControl   = 1                  ; SERVICE_ERROR_NORMAL
ServiceBinary  = %12%\FSBRLDSP.sys
;LoadOrderGroup = Base

[FSBRLDSP.AddRegNt]
HKR,,DevLoader,,*ntkern
HKR,,NTMPDriver,,FSBRLDSP.sys

[DriverCopyFiles]
FSBRLDSP.sys

