<?xml version="1.0" encoding="utf-8" ?>
<!-- Configuration file for LiblouisTrans.dll -->
<!-- Note that modes are not necessarily listed in order, you must find an unused ID to add a new mode. -->
<LiblouisTrans>
    
    <!--
        Options are global settings that apply to all modes.
        Path values are relative to the directory of the translator.
    -->
    <Options>
        <!--
            Relative path to the directory in which the Tables
            defined for each Mode can be found.
        -->
        <TablesPath>Liblouis\3.33.0\tables</TablesPath>
        
        <!--
            Relative path to the directory in which the Symbols
            defined for each Mode can be found.
        -->
        <SymbolsPath>Liblouis\3.33.0\symbols</SymbolsPath>
        
        <!--
            If enabled, Liblouis itself will generate a logfile of its
            actions in the current directory.
        -->
        <EnableLiblouisLogging>false</EnableLiblouisLogging>
        
        <!--
            If enabled, translation output will be in Unicode Braille
            format, instead of text.
        -->
        <EnableUnicodeBraille>true</EnableUnicodeBraille>

        <NoUndefinedDots>true</NoUndefinedDots>
        <PartialTrans>false</PartialTrans>
        
        <!--
            If nonempty, will force the translator to use the defined
            language.
        -->
        <LanguageOverride></LanguageOverride>
    </Options>
    
    <!--
        Each Mode element defines an available mode for the Translator
        that is associated with a particular language.
        Mode specific settings should be defined here.
    -->
    <Modes>
        <!-- 
            The order of these modes determines the order of appearance
            in the translation modes combo box.  The Id attribute is an
            arbitrary numerical identifier that must be unique.
